0) {\r\n      setSlideRows(results)\r\n    } else {\r\n      setSlideRows(slides[category])\r\n    }\r\n  }, [searchTerm])\r\n\r\n  return profile.displayName ? (\r\n    <>\r\n      {loading ? <Loading src={user.photoURL} /> : <Loading.ReleaseBody />}\r\n      <Header src=\"joker1\" dontShowOnSmallViewPort>\r\n        <Header.Frame>\r\n          <Header.Group>\r\n            <Header.Logo to={ROUTES.HOME} src=\"/images/misc/logo.svg\" alt=\"Netflix\" />\r\n            <Header.Link\r\n              active={category === 'series' ? true : false}\r\n              onClick={() => setCategory('series')}\r\n            >Series</Header.Link>\r\n            <Header.Link\r\n              active={category === 'films' ? true : false}\r\n              onClick={() => setCategory('films')}\r\n            >Films</Header.Link>\r\n          </Header.Group>\r\n          <Header.Group>\r\n            <Header.Search searchTerm={searchTerm} setSearchTerm={setSearchTerm}></Header.Search>\r\n            <Header.Profile>\r\n              <Header.Picture src={user.photoURL} alt=\"profile picture\" />\r\n              <Header.Dropdown>\r\n                <Header.Group>\r\n                  <Header.Picture src={user.photoURL} alt=\"profile picture\" />\r\n                  <Header.Link>{user.displayName}</Header.Link>\r\n                </Header.Group>\r\n                <Header.Group>\r\n                  <Header.Link onClick={() => firebase.auth().signOut()}>Sign Out</Header.Link>\r\n                </Header.Group>\r\n              </Header.Dropdown>\r\n            </Header.Profile>\r\n          </Header.Group>\r\n        </Header.Frame>\r\n\r\n        <Header.Feature>\r\n          <Header.FeatureCallOut>Watch Joker Now</Header.FeatureCallOut>\r\n          <Header.Text>Forever alone in a crowd, failed comedian Arthur Fleck seeks connection as he walks the streets of Gotham City. Arthur wears two masks -- the one he paints for his day job as a clown, and the guise he projects in a futile attempt to feel like he's part of the world around him.</Header.Text>\r\n          <Header.PlayButton>Play</Header.PlayButton>\r\n        </Header.Feature>\r\n      </Header>\r\n\r\n      <Card.Group>\r\n        {slideRows.map((slideItem) => (\r\n          <Card key={`${category}-${slideItem.title.toLowerCase()}`}>\r\n            <Card.Title>{slideItem.title}</Card.Title>\r\n            <Card.Entities>\r\n              {slideItem.data.map((item) => (\r\n                <Card.Item key={item.docId} item={item}>\r\n                  <Card.Image alt={item.title} src={`/images/${category}/${item.genre}/${item.slug}/small.jpg`} />\r\n                  <Card.Meta>\r\n                    <Card.SubTitle>{item.title}</Card.SubTitle>\r\n                    <Card.Text>{item.description}</Card.Text>\r\n                  </Card.Meta>\r\n                </Card.Item>\r\n              ))}\r\n            </Card.Entities>\r\n            <Card.Feature category={category}>\r\n              <Player>\r\n                <Player.Button />\r\n                <Player.Video />\r\n              </Player>\r\n            </Card.Feature>\r\n          </Card>\r\n        ))}\r\n      </Card.Group>\r\n      <FooterContainer />\r\n    </>\r\n  ) : (\r\n      <SelectProfileContainer user={user} setProfile={setProfile} />\r\n    )\r\n}","C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\containers\\profiles.js",[],"C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\components\\profiles\\index.js",[],"C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\components\\profiles\\styles\\profiles.js",[],"C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\components\\loading\\index.js",[],"C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\components\\loading\\styles\\loading.js",[],"C:\\Users\\Matthew\\Documents\\Web Projects\\front end\\react-netflix-clone\\src\\